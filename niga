getgenv().Settings =  {
    --// !Main! \\--
    ['USERS'] = {OWNER = 'Milan_HR'}, -- Username Of Owner
    ['Position'] = 'Back', -- Position Of Stand [Back / Left / Right]
    ['Attack'] = 'Heavy!', -- Heavy = Super Punch, Light = Spam Punch [Heavy! / Light!]
    ['CrewID'] = 10598253, -- Must Be In Group For It To Work 
    ['StandMode'] = 'Star Platinum', -- [Star Platinum / King Crimson / Golden Experience / TheWorld / MIH / Cmoon / KillerQueen / Whitesnake] put nothing in here if u dont want the sounds
    ['FlyModeAnim'] = 'Glide', -- [Glide / Fly / Hover]

    --// ! Misc ! \\--
    ['ChatCmds'] = true, -- chat when a command is used 
    ['FPSCAP'] = false, -- [Off / Number] put a number in, this is so your main account gets the most fps (MAXED IS 60, SUGGEST 50 FPS ON STANDS)
    ['LowGraphics'] = true, -- Turns graphics down so your stand doesnt lag too much
    ['NoFace'] = false, -- For A Better Look On Your Stand
    ['AutoMask'] = false, -- auto wears a mask
    ['AutoSize'] = false, -- auto changes your size to default da hood scale size
    ['NoLegs'] = false, -- makes your legs invisible (FE)
    ['TITAN'] = false, -- makes you titan (MUST BE FAT) (FE)

    --//    ! Sounds !     \\--
    --\\ MUST HAVE BOOMBOX //--
    ['CustomSummonText'] = 'Star Platinum!', -- enter your own summon text e.g "Summon!" (can effect stand gui if is changed)
    ['Sounds'] = true, -- [On / Off] Sounds when you use some commands like Mimic! Evolve! etc
    ['SummonMusicID'] = 'Default', -- Enter A Sound ID Put [Default] If You Would Like The Default Sound
    ['SummonMusic'] = true, -- [On / Off]
    ['CustomSong'] = "" -- Enter A Song ID And Say "/e song" to play it 
}



function Notif(text, time)
game.StarterGui:SetCore("SendNotification", {
Title = "Nova Stand"; 
Text = text;
Icon = "rbxassetid://7413731076";
Duration = time;
})
end


