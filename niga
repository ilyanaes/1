function OGSize()
	game.Players.LocalPlayer.Character.Humanoid.BodyDepthScale:Destroy()
	wait()
	game.Players.LocalPlayer.Character.Humanoid.BodyWidthScale:Destroy()
	wait()
	game.Players.LocalPlayer.Character.Humanoid.BodyHeightScale:Destroy()
end



function alertsound()
	local alertsound = Instance.new("Sound", game.CoreGui)
	alertsound.PlaybackSpeed = 1
	alertsound.Volume = 2
	alertsound.SoundId = "rbxassetid://9016994104"
	alertsound.PlayOnRemove = true
	alertsound:Destroy()
end

