	--anti tp bypass
	grm = getrawmetatable(game)
	setreadonly(grm, false)
	old = grm.__namecall
	grm.__namecall = newcclosure(function(self, ...)
		local args = {...}
		if tostring(args[1]) == "TeleportDetect" then
			return
		elseif tostring(args[1]) == "CHECKER_1" then
			return
		elseif tostring(args[1]) == "CHECKER" then
			return
		end
		return old(self, ...)
	end)
